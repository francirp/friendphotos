<h1>New Friend</h1>


<%= form_tag(friends_url, method: 'post') do %>


  <div>
    <%= label_tag :name %><br />
    <%= text_field_tag :name %>
  </div>

    <div>
    <%= label_tag :uid %><br />
    <%= number_field_tag :uid %>
  </div>

  
  <div>
    <%= submit_tag "Create Friend" %>
  </div>
<% end %>
